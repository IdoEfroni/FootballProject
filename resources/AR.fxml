<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>

<BorderPane xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="presentationLayer.ARController">
   <left>
      <Pane BorderPane.alignment="CENTER">
         <children>
            <Pane fx:id="arHomeMenuPane" prefHeight="666.0" prefWidth="205.0">
               <children>
                  <Button layoutX="23.0" layoutY="43.0" mnemonicParsing="false" onAction="#switchApproveCreate" text="approve/create team" />
                  <Button layoutX="24.0" layoutY="107.0" mnemonicParsing="false" prefHeight="31.0" prefWidth="156.0" text="preferences setting" />
                  <Button layoutX="70.0" layoutY="233.0" mnemonicParsing="false" text="back" />
               </children>
            </Pane>
            <Pane fx:id="approveOrCreatePane" prefHeight="562.0" prefWidth="200.0" visible="false">
               <children>
                  <Label layoutX="50.0" layoutY="34.0" text="approve/create" />
                  <Button layoutX="68.0" layoutY="80.0" mnemonicParsing="false" onAction="#switchApprove" text="approve" />
                  <Button layoutX="68.0" layoutY="143.0" mnemonicParsing="false" onAction="#switchCreate" text="create" />
                  <Button layoutX="68.0" layoutY="205.0" mnemonicParsing="false" text="back" />
               </children>
            </Pane>
         </children>
      </Pane>
   </left>
   <center>
      <Pane fx:id="approveCreatePane" prefHeight="530.0" prefWidth="2545.0" BorderPane.alignment="CENTER">
         <children>
            <Pane fx:id="createTeamPane" layoutX="1.0" prefHeight="666.0" prefWidth="620.0" visible="false">
               <children>
                  <Button layoutX="79.0" layoutY="70.0" mnemonicParsing="false" text="create" />
                  <ComboBox layoutX="209.0" layoutY="70.0" prefWidth="150.0" promptText="choose team" />
               </children></Pane>
            <Pane fx:id="approveTeamPane" prefHeight="579.0" prefWidth="658.0" visible="false">
               <children>
                  <Label layoutX="87.0" layoutY="39.0" />
                  <Button layoutX="260.0" layoutY="34.0" mnemonicParsing="false" text="approve" />
                  <ComboBox layoutX="48.0" layoutY="34.0" prefWidth="150.0" promptText="choose team" />
               </children>
            </Pane>
         </children></Pane>
   </center>
</BorderPane>

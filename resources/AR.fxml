                  <Label layoutX="45.0" layoutY="4.0" text="Hello:" />
               </children>
            </Pane>
         </children>
      </Pane>
   </left>
   <center>
      <Pane fx:id="approveCreatePane" prefHeight="530.0" prefWidth="2545.0" BorderPane.alignment="CENTER">
         <children>
            <Pane fx:id="createTeamPane" layoutX="1.0" prefHeight="666.0" prefWidth="620.0" visible="false">
               <children>
                  <Button layoutX="79.0" layoutY="70.0" mnemonicParsing="false" text="create" />
                  <ComboBox layoutX="209.0" layoutY="70.0" prefWidth="150.0" promptText="choose team" />
               </children></Pane>
            <Pane fx:id="approveTeamPane" prefHeight="579.0" prefWidth="658.0" visible="false">
               <children>
                  <Label layoutX="87.0" layoutY="39.0" />
                  <Button layoutX="260.0" layoutY="34.0" mnemonicParsing="false" text="approve" />
                  <ComboBox layoutX="48.0" layoutY="34.0" prefWidth="150.0" promptText="choose team" />
               </children>
            </Pane>
            <Pane fx:id="createLeaguePane" prefHeight="680.0" prefWidth="1749.0" visible="false">
               <children>
                  <Button layoutX="344.0" layoutY="133.0" mnemonicParsing="false" onAction="#createLeague" text="create league" />
                  <TextField fx:id="leagueIdField" layoutX="106.0" layoutY="133.0" promptText="choose league name" />
               </children></Pane>
            <Pane fx:id="createSeasonPane" layoutX="1.0" prefHeight="680.0" prefWidth="1405.0">
               <children>
                  <DatePicker fx:id="endingDate" layoutX="617.0" layoutY="77.0" prefHeight="31.0" prefWidth="135.0" promptText="ending date" />
                  <DatePicker fx:id="startingDate" layoutX="435.0" layoutY="77.0" prefHeight="31.0" prefWidth="148.0" promptText="starting date" />
                  <ComboBox fx:id="leagueCombo" layoutX="33.0" layoutY="77.0" prefHeight="31.0" prefWidth="201.0" promptText="choose league" />
                  <Spinner fx:id="winSpinner" layoutX="272.0" layoutY="169.0" prefHeight="31.0" prefWidth="123.0" />
                  <Spinner fx:id="loseSpinner" layoutX="441.0" layoutY="169.0" prefHeight="31.0" prefWidth="136.0" />
                  <Spinner fx:id="tieSpinner" layoutX="619.0" layoutY="169.0" prefHeight="31.0" prefWidth="130.0" />
                  <Pane prefHeight="543.0" prefWidth="718.0" visible="false" />
                  <Spinner fx:id="seasonSpinner" layoutX="268.0" layoutY="77.0" prefHeight="31.0" prefWidth="128.0" />
                  <Label layoutX="300.0" layoutY="49.0" text="season ID" />
                  <Label layoutX="281.0" layoutY="141.0" text="Win score policy" />
                  <Label layoutX="448.0" layoutY="141.0" text="Lose score policy" />
                  <Label layoutX="629.0" layoutY="141.0" text="Tie score policy" />
                  <Label layoutX="121.0" layoutY="48.0" />
                  <ComboBox fx:id="policyCombo" layoutX="33.0" layoutY="169.0" prefHeight="31.0" prefWidth="202.0" promptText="choose match policy" />
                  <Label layoutX="459.0" layoutY="48.0" text="Starting Date" />
                  <Label layoutX="634.0" layoutY="49.0" text="Ending Date" />
                  <Button layoutX="33.0" layoutY="247.0" mnemonicParsing="false" onAction="#createSeason" prefHeight="31.0" prefWidth="202.0" text="create season" />
               </children></Pane>
         </children></Pane>
   </center>
</BorderPane>

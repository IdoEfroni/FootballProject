<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<BorderPane prefHeight="666.0" prefWidth="1166.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="presentationLayer.TeamOwnerController">
    <top>
        <HBox id="actions" prefHeight="52.0" prefWidth="1100.0" spacing="10.0">
            <children>
                <Button minHeight="30.0" minWidth="140.0" mnemonicParsing="false" onAction="#addNewTeam" prefHeight="30.0" prefWidth="295.0" text="View Assigned games " textAlignment="CENTER">
                    <HBox.margin>
                        <Insets left="5.0" />
                    </HBox.margin>
                </Button>
            <MenuButton minHeight="30.0" minWidth="145.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="305.0" text="Game Management ">
              <items>
                <MenuItem mnemonicParsing="false" text="Report Foul" />
                <MenuItem mnemonicParsing="false" text="Report Goal" />
                  <MenuItem mnemonicParsing="false" text="Report Injury" />
                  <MenuItem mnemonicParsing="false" text="Report Offside" />
                  <MenuItem mnemonicParsing="false" text="Report Red Card" />
                  <MenuItem mnemonicParsing="false" text="Report Yellow Card" />
                  <MenuItem mnemonicParsing="false" text="Report Substitution" />
              </items>
            </MenuButton>
                <Label fx:id="userLable" alignment="TOP_CENTER" centerShape="false" contentDisplay="RIGHT" focusTraversable="false" minWidth="173.0" prefHeight="21.0" prefWidth="173.0" text="Welcome &lt;username&gt;" textAlignment="CENTER">
                    <HBox.margin>
                        <Insets left="650.0" />
                    </HBox.margin>
                    <font>
                        <Font name="System Italic" size="15.0" />
                    </font>
                    <padding>
                        <Insets top="5.0" />
                    </padding>
                </Label>
                <Button minHeight="30.0" minWidth="65.0" mnemonicParsing="false" onAction="#logoutB" prefHeight="30.0" prefWidth="74.0" text="Logout">
                    <HBox.margin>
                        <Insets right="20.0" />
                    </HBox.margin></Button>
            </children>
            <padding>
                <Insets bottom="10.0" left="20.0" top="10.0" />
            </padding>
        </HBox>
    </top>
    <left>
        <VBox id="notifications" alignment="CENTER" prefHeight="623.0" prefWidth="244.0">
            <children>
                <Label disable="true" text="&lt;No new notifications&gt;">
                    <font>
                        <Font name="System Italic" size="12.0" />
                    </font>
                </Label>
            </children>
        </VBox>
    </left>
    <center>
        <BorderPane prefHeight="614.0" prefWidth="771.0" BorderPane.alignment="CENTER">
            <top>
                <HBox prefHeight="61.0" prefWidth="922.0" BorderPane.alignment="CENTER">
                    <children>
                        <Label fx:id="titleL">
                            <font>
                                <Font size="25.0" />
                            </font>
                        </Label>
                    </children>
                    <padding>
                        <Insets left="50.0" top="10.0" />
                    </padding>
                </HBox>
            </top>
            <center>
                <Pane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
            </center>
        </BorderPane>
    </center>
</BorderPane>

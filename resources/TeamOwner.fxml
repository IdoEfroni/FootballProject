<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<BorderPane prefHeight="666.0" prefWidth="1166.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="presentationLayer.TeamOwnerController">
   <top>
      <HBox id="actions" prefHeight="52.0" prefWidth="1088.0" spacing="10.0">
         <children>
            <Button mnemonicParsing="false" onAction="#addNewTeam" prefHeight="30.0" prefWidth="100.0" text="Add New Team" textAlignment="CENTER">
               <HBox.margin>
                  <Insets left="5.0" />
               </HBox.margin>
            </Button>
            <Button mnemonicParsing="false" prefHeight="30.0" prefWidth="76.0" text="Button">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </Button>
            <Label fx:id="userLable" alignment="TOP_CENTER" centerShape="false" contentDisplay="RIGHT" focusTraversable="false" prefHeight="21.0" prefWidth="173.0" text="Welcome &lt;username&gt;" textAlignment="CENTER">
               <HBox.margin>
                  <Insets left="650.0" />
               </HBox.margin>
               <font>
                  <Font name="System Italic" size="15.0" />
               </font>
               <padding>
                  <Insets top="5.0" />
               </padding>
            </Label>
            <Button mnemonicParsing="false" onAction="#logoutB" prefHeight="30.0" prefWidth="60.0" text="Logout" />
         </children>
         <padding>
            <Insets bottom="10.0" left="20.0" top="10.0" />
         </padding>
      </HBox>
   </top>
   <left>
      <VBox id="notifications" alignment="CENTER" prefHeight="623.0" prefWidth="244.0">
         <children>
            <Label disable="true" text="&lt;No new notifications&gt;">
               <font>
                  <Font name="System Italic" size="12.0" />
               </font>
            </Label>
         </children>
      </VBox>
   </left>
   <center>
      <BorderPane prefHeight="614.0" prefWidth="771.0" BorderPane.alignment="CENTER">
         <top>
            <HBox prefHeight="61.0" prefWidth="922.0" BorderPane.alignment="CENTER">
               <children>
                  <Label fx:id="titleL">
                     <font>
                        <Font size="25.0" />
                     </font>
                  </Label>
               </children>
               <padding>
                  <Insets left="50.0" top="10.0" />
               </padding>
            </HBox>
         </top>
         <center>
            <Pane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
               <children>
                  <Pane fx:id="newTeamPane" prefHeight="557.0" prefWidth="924.0" visible="false">
                     <children>
                        <Label layoutX="30.0" layoutY="18.0" text="Team Name:">
                           <font>
                              <Font size="15.0" />
                           </font></Label>
                        <TextField fx:id="teamNameL" layoutX="149.0" layoutY="16.0" />
                        <Label layoutX="30.0" layoutY="62.0" text="Year Established:">
                           <font>
                              <Font size="15.0" />
                           </font>
                        </Label>
                        <Spinner fx:id="yearSpinner" editable="true" layoutX="149.0" layoutY="60.0" prefHeight="25.0" prefWidth="81.0" />
                        <Button layoutX="30.0" layoutY="107.0" mnemonicParsing="false" onAction="#submitNewTeam" text="Submit" />
                     </children>
                  </Pane>
               </children>
            </Pane>
         </center>
      </BorderPane>
   </center>
</BorderPane>
